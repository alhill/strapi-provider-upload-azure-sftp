{
    "parserOptions": {
        "sourceType": "module",
        "ecmaVersion": 8,
        "ecmaFeatures": {
            "jsx": true,
            "experimentalObjectRestSpread": true
        }
    },
    "env": {
        "browser": true,
        "node": true,
        "es6": true
    },
    "rules": {
        "brace-style": [
            1,
            "stroustrup",
            {
                "allowSingleLine": true
            }
        ],
        "comma-style": [
            1,
            "last"
        ],
        "curly": [
            1,
            "all"
        ],
        "eol-last": 1,
        "eqeqeq": 1,
        "handle-callback-err": [
            1,
            "error"
        ],
        "jsx-quotes": [
            1,
            "prefer-double"
        ],
        "keyword-spacing": [
            1,
            {
                "before": true,
                "after": true
            }
        ],
        "no-labels": 1,
        "no-undef-init": 1,
        "no-unreachable": 1,
        "no-unused-expressions": 1,
        "no-unused-vars": [
            1,
            {
                "vars": "all",
                "args": "after-used"
            }
        ],
        "no-trailing-spaces": 1,
        "no-void": 1,
        "no-with": 1,
        "one-var": [
            1,
            {
                "uninitialized": "always",
                "initialized": "never"
            }
        ],
        "operator-assignment": [
            1,
            "always"
        ],
        "radix": 1,
        "semi-spacing": [
            1,
            {
                "before": false,
                "after": true
            }
        ],
        "semi": [
            1,
            "always"
        ],
        "space-before-blocks": [
            1,
            "always"
        ],
        "space-before-function-paren": [
            1,
            {
                "anonymous": "always",
                "named": "always"
            }
        ],
        "space-infix-ops": [
            1,
            {
                "int32Hint": false
            }
        ],
        "space-unary-ops": [
            1,
            {
                "words": true,
                "nonwords": false
            }
        ],
        "use-isnan": 1,
        "valid-typeof": 1,
        "wrap-iife": 1,
        "yoda": [
            1,
            "never",
            {
                "exceptRange": true
            }
        ],
        "indent": [
            1,
            4,
            {
                "SwitchCase": 1
            }
        ],
        "key-spacing": [
            1,
            {
                "beforeColon": false,
                "afterColon": true
            }
        ],
        "new-parens": 1,
        "no-alert": 1,
        "no-array-constructor": 1,
        "no-caller": 1,
        "no-catch-shadow": 1,
        "no-cond-assign": 1,
        "no-constant-condition": 1,
        "no-delete-var": 1,
        "no-div-regex": 1,
        "no-dupe-args": 1,
        "no-dupe-keys": 1,
        "no-duplicate-case": 1,
        "no-empty-character-class": 1,
        "no-empty": 1,
        "no-eval": 1,
        "no-ex-assign": 1,
        "no-extend-native": 1,
        "no-extra-bind": 1,
        "no-extra-boolean-cast": 1,
        "no-extra-semi": 1,
        "no-fallthrough": 1,
        "no-floating-decimal": 1,
        "no-func-assign": 1,
        "no-implied-eval": 1,
        "no-inner-declarations": [
            1,
            "functions"
        ],
        "no-invalid-regexp": 1,
        "no-irregular-whitespace": 1,
        "no-iterator": 1,
        "no-label-var": 1,
        "no-lonely-if": 1,
        "no-mixed-requires": [
            1,
            true
        ],
        "no-mixed-spaces-and-tabs": 1,
        "no-multi-spaces": 1,
        "no-multi-str": 1,
        "no-negated-in-lhs": 1,
        "no-new-object": 1,
        "no-new-require": 1,
        "no-new-wrappers": 1,
        "no-new": 1,
        "no-obj-calls": 1,
        "no-octal-escape": 1,
        "no-octal": 1,
        "no-param-reassign": 1,
        "no-path-concat": 1,
        "no-proto": 1,
        "no-redeclare": 1,
        "no-regex-spaces": 1,
        "no-return-assign": 1,
        "no-script-url": 1,
        "no-sequences": 1,
        "no-shadow-restricted-names": 1,
        "no-shadow": 1,
        "no-spaced-func": 1,
        "no-sparse-arrays": 1,
        "no-sync": 1,
        "no-throw-literal": 1,
        "no-undef": 1,
        "object-curly-spacing": [
            1,
            "always",
            {
                "objectsInObjects": true,
                "arraysInObjects": true
            }
        ],
        "quote-props": [
            1,
            "as-needed"
        ],
        "quotes": [
            1,
            "single",
            "avoid-escape"
        ]
    }
}
